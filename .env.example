# Copy this file to .env and fill with your values
#
# BARQOUQ_GRPC_HOST: Barqouq gRPC endpoint (host:port)
# BARQOUQ_GRPC_TLS:  "true" to use TLS, "false" for insecure
# BARQOUQ_SECRET_KEY: Your Barqouq client secret key
# BARQOUQ_SUBDOMAIN: Your Barqouq shop subdomain

BARQOUQ_GRPC_HOST=api.barqouq.shop:443
BARQOUQ_GRPC_TLS=true
BARQOUQ_SECRET_KEY=
BARQOUQ_SUBDOMAIN=
